{
  "name": "aou-puppeteer-test-automation",
  "description": "All-of-Us end-to-end test automation framework",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "BSD",
  "scripts": {
    "create-signin-token-dir": "mkdir -p ../e2e/signin-tokens",
    "impersonate-test-user-owner": "env-cmd -x ../api/project.rb generate-impersonated-user-token --impersonated-user=\\$USER_NAME --output-token-filename=../e2e/signin-tokens/puppeteer-access-token.txt",
    "impersonate-test-reader-user": "env-cmd -x ../api/project.rb generate-impersonated-user-token --impersonated-user=\\$READER_USER --output-token-filename=../e2e/signin-tokens/reader-puppeteer-access-token.txt",
    "impersonate-test-writer-user": "env-cmd -x ../api/project.rb generate-impersonated-user-token --impersonated-user=\\$WRITER_USER --output-token-filename=../e2e/signin-tokens/writer-puppeteer-access-token.txt",
    "impersonate-test-collaborator-user": "env-cmd -x ../api/project.rb generate-impersonated-user-token --impersonated-user=\\$COLLABORATOR_USER --output-token-filename=../e2e/signin-tokens/collaborator-puppeteer-access-token.txt",
    "impersonate-test-users": "yarn create-signin-token-dir && yarn impersonate-test-user-owner && yarn impersonate-test-reader-user && yarn impersonate-test-writer-user && yarn impersonate-test-collaborator-user",
    "_test": "yarn impersonate-test-users && jest --no-color",
    "_test:debugTest": "yarn impersonate-test-users && node --inspect-brk node_modules/.bin/jest --no-color",
    "test": "cross-env WORKBENCH_ENV=test yarn _test --maxWorkers=5",
    "test-perf": "cross-env WORKBENCH_ENV=perf yarn _test",
    "test-perf:debug": "cross-env PUPPETEER_HEADLESS=false WORKBENCH_ENV=perf yarn _test --maxWorkers=1",
    "test:debugTest": "cross-env PUPPETEER_DEBUG=true yarn _test:debugTest",
    "test:debug": "cross-env PUPPETEER_DEBUG=true yarn _test --detectOpenHandles --maxWorkers=1",
    "test-local": "cross-env WORKBENCH_ENV=local DEBUG=true yarn _test --maxWorkers=3",
    "test-local:debug": "cross-env WORKBENCH_ENV=local PUPPETEER_DEBUG=true yarn _test --maxWorkers=1",
    "test-local-devup": "cross-env WORKBENCH_ENV=local DEBUG=true jest --maxWorkers=3",
    "test-local-devup:debug": "cross-env WORKBENCH_ENV=local PUPPETEER_DEBUG=true node --inspect-brk node_modules/.bin/jest --maxWorkers=1",
    "test-staging": "cross-env WORKBENCH_ENV=staging yarn _test --maxWorkers=3",
    "test-staging:debug": "cross-env WORKBENCH_ENV=staging PUPPETEER_DEBUG=true yarn _test --maxWorkers=1",
    "test:ci": "yarn _test --ci --maxWorkers=3 --forceExit",
    "test:ci:debug": "yarn _test --ci --debug --maxWorkers=3",
    "test-nightly": "cross-env WORKBENCH_ENV=test TEST_MODE=nightly-integration yarn _test --maxWorkers=2",
    "test-nightly:debug": "cross-env PUPPETEER_HEADLESS=false WORKBENCH_ENV=test TEST_MODE=nightly-integration yarn _test --detectOpenHandles",
    "test-nightly-local-devup": "cross-env WORKBENCH_ENV=local TEST_MODE=nightly-integration yarn jest --maxWorkers=1",
    "test-nightly-local-devup:debug": "cross-env WORKBENCH_ENV=local TEST_MODE=nightly-integration PUPPETEER_HEADLESS=false DEBUG=true yarn jest --maxWorkers=1",
    "clean": "rimraf tsc-out",
    "build": "yarn clean && tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config .prettierrc.js --write --ignore-unknown \"**/*.{ts,js}\""
  },
  "keywords": [
    "test",
    "puppeteer",
    "headless",
    "chrome",
    "ui"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.5",
    "@types/faker": "^5.5.6",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/jest-environment-puppeteer": "^4.4.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/puppeteer": "^5.4.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "jest-config": "^27.0.6",
    "jest-junit": "^12.2.0",
    "jest-puppeteer": "^4.4.0",
    "jest-stare": "^2.2.2",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.1.2",
    "winston": "^3.3.3"
  }
}
