{
  "name": "aou-puppeteer-test-automation",
  "description": "All-of-Us end-to-end test automation framework",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "BSD",
  "scripts": {
    "_create-signin-token-dir": "mkdir -p ../e2e/signin-tokens",
    "_impersonate-test-users": "yarn _create-signin-token-dir && yarn install && env-cmd -x ../api/project.rb generate-impersonated-user-tokens --impersonated-usernames \\$USER_NAME,\\$READER_USER,\\$WRITER_USER,\\$ACCESS_TEST_USER,\\$ADMIN_TEST_USER,\\$EGRESS_TEST_USER --output-token-dir ../e2e/signin-tokens",
    "_test": "node -r dotenv/config . && yarn _impersonate-test-users && jest --no-color",
    "_test:debugTest": "node -r dotenv/config . && yarn _impersonate-test-users && node --inspect-brk node_modules/.bin/jest --no-color",
    "test": "cross-env WORKBENCH_ENV=test yarn _test --maxWorkers=5",
    "test:debugTest": "cross-env WORKBENCH_ENV=test PUPPETEER_DEBUG=true yarn _test:debugTest",
    "test:debug": "cross-env WORKBENCH_ENV=test PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false yarn _test --detectOpenHandles --maxWorkers=1",
    "test:ci": "yarn _test --ci --maxWorkers=2 --forceExit",
    "test-local": "cross-env WORKBENCH_ENV=local DEBUG=true yarn _test --maxWorkers=3",
    "test-local:debug": "cross-env WORKBENCH_ENV=local PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false yarn _test --maxWorkers=1",
    "test-nightly": "cross-env WORKBENCH_ENV=test TEST_MODE=nightly-integration yarn _test --maxWorkers=2",
    "test-nightly:debug": "cross-env PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false WORKBENCH_ENV=test TEST_MODE=nightly-integration yarn _test --detectOpenHandles",
    "test-nightly-staging:debug": "cross-env WORKBENCH_ENV=staging PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false TEST_MODE=nightly-integration yarn _test --maxWorkers=1",
    "test-local-nightly": "cross-env WORKBENCH_ENV=local TEST_MODE=nightly-integration yarn _test --maxWorkers=3",
    "test-local-nightly:debug": "cross-env PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false WORKBENCH_ENV=local TEST_MODE=nightly-integration yarn _test --detectOpenHandles",
    "test-perf": "cross-env WORKBENCH_ENV=perf yarn _test",
    "test-perf:debug": "cross-env PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false WORKBENCH_ENV=perf yarn _test --maxWorkers=1",
    "test-staging": "cross-env WORKBENCH_ENV=staging yarn _test --maxWorkers=2",
    "test-staging:debug": "cross-env WORKBENCH_ENV=staging PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false yarn _test --maxWorkers=1",
    "test-staging-nightly": "cross-env WORKBENCH_ENV=staging TEST_MODE=nightly-integration yarn _test --maxWorkers=2",
    "test-staging-nightly:debug": "cross-env WORKBENCH_ENV=staging PUPPETEER_DEBUG=true PUPPETEER_HEADLESS=false TEST_MODE=nightly-integration yarn _test --maxWorkers=1",
    "clean": "rimraf tsc-out",
    "build": "yarn clean && tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config .prettierrc.js --write --ignore-unknown \"**/*.{ts,js}\""
  },
  "keywords": [
    "test",
    "puppeteer",
    "headless",
    "chrome",
    "ui"
  ],
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.5",
    "@types/faker": "^5.5.6",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^27.0.2",
    "@types/jest-environment-puppeteer": "^4.4.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/puppeteer": "^5.4.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "jest-config": "^27.2.5",
    "jest-junit": "^13.0.0",
    "jest-puppeteer": "^5.0.4",
    "jest-stare": "^2.3.0",
    "lodash": "^4.17.20",
    "otplib": "12.0.0",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.1.2",
    "wait-for-expect": "^3.0.2",
    "winston": "^3.3.3"
  }
}
