{
  "name": "aou-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev-up": "yarn run codegen && yarn start",
    "build": "ng build",
    "test": "ng test --source-map=false",
    "test-react": "jest",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run merge-swagger && yarn run codegen-clean && yarn run codegen-swagger && yarn run codegen-swagger-fetch && yarn run post-codegen && yarn run notebooks-codegen-swagger && yarn run notebooks-codegen-swagger-fetch && yarn run notebooks-post-codegen",
    "merge-swagger": "../api/build.rb merge-yaml --no-docker",
    "codegen-clean": "rm -rf src/generated src/notebooks-generated",
    "codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/merged.yaml --output src/generated --additional-properties ngVersion=2",
    "codegen-swagger-fetch": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-fetch --input-spec ../api/src/main/resources/merged.yaml --output src/generated/fetch",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "notebooks-codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/notebooks.yaml --output src/notebooks-generated --additional-properties ngVersion=2",
    "notebooks-codegen-swagger-fetch": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-fetch --input-spec ../api/src/main/resources/notebooks.yaml --output src/notebooks-generated/fetch",
    "notebooks-post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/notebooks-generated/variables.ts",
    "coverage": "ng test --source-map=false --watch=false --code-coverage"
  },
  "jest": {
    "setupFiles": [
      "./test-shim.js",
      "./test-setup.js"
    ],
    "setupTestFrameworkScriptFile": "./test-setup-script.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "spec.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "src/tsconfig.jest.json"
      }
    }
  },
  "browserslist": [
    "chrome >= 67"
  ],
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^6.6.0",
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "7.1.3",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@clr/angular": "^0.11.12",
    "@clr/icons": "^0.11.12",
    "@clr/ui": "^0.11.12",
    "@types/color": "^3.0.0",
    "@types/core-js": "^2.5.0",
    "@types/geojson": "^1.0.6",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.7.12",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "^3.8.0",
    "@types/react-onclickoutside": "^6.7.0",
    "@types/tough-cookie": "^2.3.5",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular2-highcharts": "^0.5.5",
    "classnames": "^2.2.6",
    "color": "^3.1.1",
    "core-js": "^2.5.3",
    "d3": "^4.12.2",
    "highcharts": "^6.2.0",
    "highcharts-react-official": "^2.0.0",
    "immutable": "^4.0.0-rc.9",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mutationobserver-shim": "^0.3.2",
    "ng2-nouislider": "^1.7.7",
    "ngx-popper": "^5.1.7",
    "nouislider": "10.0.0",
    "outdated-browser-rework": "^2.8.0",
    "popper.js": "^1.13.0",
    "portable-fetch": "^3.0.0",
    "primeicons": "^1.0.0",
    "primereact": "^3.1.1",
    "react": "^16.6.3",
    "react-addons-test-utils": "^15.6.2",
    "react-calendar": "^2.18.1",
    "react-dom": "^16.6.3",
    "react-interactive": "^0.8.1",
    "react-measure": "^2.2.3",
    "react-modal": "^3.8.1",
    "react-onclickoutside": "^6.7.1",
    "react-paginating": "^1.0.3-react16.0",
    "react-select": "^2.3.0",
    "react-switch": "^4.1.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.5.6",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.0",
    "validate.js": "^0.12.0",
    "yarn": "^1.12.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/core": "7.1.0",
    "@angular-devkit/schematics": "7.1.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/enzyme": "^3.1.15",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.11",
    "codelyzer": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-webdriver-launcher": "^1.0.5",
    "lolex": "^3.0.0",
    "protractor": "~5.4.1",
    "react-test-renderer": "^16.6.3",
    "ts-node": "~3.0.4",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": ">=3.1.1 <3.2"
  }
}
