{
  "name": "aou-ui-angular",
  "version": "0.0.2",
  "license": "BSD",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "codegen": "yarn run merge-swagger && yarn run codegen-clean && yarn run codegen-swagger && yarn run post-codegen && yarn run notebooks-codegen-swagger && yarn run notebooks-post-codegen",
    "merge-swagger": "../api/build.rb merge-yaml",
    "codegen-clean": "rm -rf src/generated src/notebooks-generated",
    "codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/merged.yaml --output src/generated --additional-properties ngVersion=2",
    "post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/generated/variables.ts",
    "notebooks-codegen-swagger": "java -jar ../aou-utils/swagger-codegen-cli.jar generate --lang typescript-angular --input-spec ../api/src/main/resources/notebooks.yaml --output src/notebooks-generated --additional-properties ngVersion=2",
    "notebooks-post-codegen": "sed -i.bak -e 's/OpaqueToken/InjectionToken/' src/notebooks-generated/variables.ts",
    "coverage": "ng test --sourcemaps=false --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^6.6.0",
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@clr/angular": "^0.11.12",
    "@clr/icons": "^0.11.12",
    "@clr/ui": "^0.11.12",
    "@swimlane/ngx-charts": "^7.1.1",
    "@types/geojson": "^1.0.6",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular-2-local-storage": "^1.0.1",
    "angular2-highcharts": "^0.5.5",
    "bootstrap": "4.1.2",
    "core-js": "^2.5.3",
    "d3": "^4.12.2",
    "dom-to-image": "^2.6.0",
    "enhanced-resolve": "3.3.0",
    "highcharts": "^6.2.0",
    "immutable": "^4.0.0-rc.9",
    "moment": "^2.20.1",
    "mutationobserver-shim": "^0.3.2",
    "ng2-nouislider": "^1.7.7",
    "ngx-popper": "^5.1.7",
    "nouislider": "10.0.0",
    "popper.js": "^1.13.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.5.6",
    "stackdriver-errors-js": "^0.4.0",
    "stacktrace-js": "^2.0.0",
    "tslib": "^1.9.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/core": "7.1.0",
    "@angular-devkit/schematics": "7.1.0",
    "@angular/cli": "^7.1.0",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^10.12.11",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-webdriver-launcher": "^1.0.5",
    "protractor": "~5.4.1",
    "ts-node": "~3.0.4",
    "tslint": "^5.9.1",
    "typescript": ">=3.1.1 <3.2"
  }
}
