<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    <changeSet author="nehasa" id="changelog-183-user-recent-resource-add-resource-type-identifier">
        <addColumn tableName="user_recent_resource">
            <column name="resource_type" type="tinyint">
                <constraints nullable="false"/>
            </column>
        </addColumn>
        <addColumn tableName="user_recent_resource">
            <column name="resource_identifier" type="varchar(1024)">
                <constraints nullable="false"/>
            </column>
        </addColumn>
    </changeSet>
</databaseChangeLog>
