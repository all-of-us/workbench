<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="mtalbott" id="changelog-273-add-temporary-initial-credits-relink-workspace-table">
        <createTable tableName="temporary_initial_credits_relink_workspace">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="source_workspace_namespace" type="varchar(255)">
                <constraints nullable="false" foreignKeyName="fk_temporary_initial_credits_relink_workspace_source_workspace_namespace" references="workspace(workspace_namespace)" deleteCascade="true"/>
            </column>
            <column name="destination_workspace_namespace" type="varchar(255)">
                <constraints nullable="false" foreignKeyName="fk_temporary_initial_credits_relink_workspace_destination_workspace_namespace" references="workspace(workspace_namespace)" deleteCascade="true"/>
            </column>
            <column name="created" type="datetime">
                <constraints nullable="false"/>
            </column>
            <column name="clone_completed" type="datetime">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
